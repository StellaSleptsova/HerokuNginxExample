[{
  "selector": "node",
  "style": {
    "text-valign": "center",
    "text-halign": "center",
    "width": 60,
    "height": 60
  }
}, {
  "selector": "node[type]",
  "style": {
    "label": "data(type)"
  }
},{
  "selector": "node[label]",
  "style": {
    "label": "data(label)"
  }
}, {
  "selector": "edge",
  "style": {
    "width": 1,
    "curve-style": "bezier",
    "control-point-step-size": 40
  }
}, {
  "selector": "edge[arrow]",
  "style": {
    "target-arrow-shape": "data(arrow)"
  }
}]
